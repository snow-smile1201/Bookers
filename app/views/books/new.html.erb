<h2>Books</h2>
<p>ここにはテーブルでindexを表示？</p>
 <p>title</p>
 <p>boby</p>
<% @books.each do |book| %>
  <span><%= book.title %></span>
  <span><%= book.body %></span>
  <span><%= link_to 'show', book_path(book.id) %></span>
  <span><%= link_to "edit", edit_book_path(book.id) %></span>
<% end %>

<h4>New Book</h4>
<%= form_with model: @book do |f| %>
  <div>Title</div>
  <%= f.text_field :title %>

  <div>Body</div>
  <%= f.text_area :body %>
  <%= f.submit 'create book' %>
<% end %>