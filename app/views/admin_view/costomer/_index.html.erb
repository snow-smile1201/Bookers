<table>
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <!--商品のインスタンス変数から-->
    <% customers.each do |customer| %>
    <tr>
      <td> <%= customer.id %> </td>
      <td><%=　link_to customer.name, edit_admin_customer_path(customer) %></td>
      <td><%= customer.email %></td>
      <% if customer.is_active == 'true' %>
        <td>有効</td>
      <% else %>
        <td>退会</td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>